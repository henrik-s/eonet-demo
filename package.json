{
    "name": "eonet-demo",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prebuild": "rm -rf dist/",
        "build": "webpack --config webpack/webpack.prod.js",
        "lint": "eslint --ext .tsx,.ts,.js,.jsx src/ server/ webpack/",
        "lint-fix": "npm run lint -- --fix",
        "start": "webpack-dev-server --config webpack/webpack.dev.js",
        "prestartLocalServer": "rm -rf lib/",
        "startLocalServer": "webpack --config webpack/webpack.localServer.js",
        "startDev": "concurrently -p \" {name} | {time} \" -n \"webpack,localServer\" -c \"green,blue\" -t \"HH:mm:ss\" \"npm run start\" \"npm run startLocalServer\"",
        "test": "jest --passWithNoTests"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "author": "Henrik Sohlberg",
    "license": "ISC",
    "homepage": "",
    "devDependencies": {
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.3",
        "@types/classnames": "^2.2.10",
        "@types/express": "^4.17.3",
        "@types/jest": "^25.1.4",
        "@types/react": "16.9.23",
        "@types/react-dom": "16.9.5",
        "@types/react-test-renderer": "^16.9.2",
        "@typescript-eslint/parser": "^2.23.0",
        "babel-jest": "^25.1.0",
        "classnames": "^2.2.6",
        "concurrently": "^5.1.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-plugin-react": "^7.19.0",
        "express": "^4.17.1",
        "html-webpack-plugin": "^4.0.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.1",
        "nodemon": "^2.0.2",
        "nodemon-webpack-plugin": "^4.3.1",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-test-renderer": "^16.13.0",
        "sass-loader": "^8.0.2",
        "sass-resources-loader": "^2.0.1",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.3",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-node-externals": "^1.7.2"
    },
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "moduleNameMapper": {
            "\\.s[ac]ss$": "identity-obj-proxy"
        },
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.test.json",
                "babelConfig": ".babelrc"
            }
        }
    }
}
